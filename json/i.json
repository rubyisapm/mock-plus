/**
 * 关于该接口的描述...
 * */
{

    //名字
    "name":{
        "first|@FIRST":"ruby",
        "last|@LAST":"huang"
    },
    //兴趣爱好
    "hobby":["roller skating","reading","swimming"],
    //所在地
    /*现居地址*/
    "address":{
        "area|@AREA":"华东",
        "region|@REGION":"浙江"
    },
    //邮箱
    /*注册邮箱*/
    "email|@EMAIL":"skysunshine99@126.com",
    //头像
    /*社区网站头像*/
    "icon|@IMAGE('200x100', '#00405d', '#FFF', 'Mock.js')":"http://www.ruby.com/ruby.png",
    //喜欢的食物
    "food|@PICK(['cookie','water','wine'])":"cookie",
    //注册日期
    "date|@DATE('yyyy-MM-dd')":"2014-01-01",
    //个人主页
    /*个人中心地址*/
    "space|@URL":"http://www.ruby.i",
    //喜欢的一句话
    "sentence|@SENTENCE(10)":"Enjoy what you have",
    //年龄
    "age|@NATURAL(10,27)":27,
    //网站权限
    /*在该网站的权限*/
    "powers":{
        //社区权限
        "community":{
            //浏览权限
            //是否可浏览社区文章
            "read|@BOOLEAN(1,6,false)":true,
            //发布权限
            /*是否可以发布文章*/
            "write":true
        },
        //管理权限
        "manage":{
            //查看用户权限
            /*是否可查看用户信息*/
            "read|10-1":true,
            //操作用户权限
            /*是否可以对用户进行增删改操作*/
            "write|1":false
        }
    }
}